# Dzine image generation configuration
# Copy to ~/.config/newproject/dzine.env and fill in values

# Dzine URLs (defaults are usually fine)
DZINE_BASE_URL=https://www.dzine.ai
DZINE_CREATE_URL=https://www.dzine.ai/tools/z-image/

# Supabase Storage bucket for generated images
DZINE_STORAGE_BUCKET=dzine-assets

# Manual-assist timeout in seconds (how long to wait for human intervention)
DZINE_MANUAL_ASSIST_TIMEOUT=300

# Generation timeout in milliseconds (max wait for Dzine to produce an image)
DZINE_GENERATION_TIMEOUT=180000
